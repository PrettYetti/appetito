<div class="col-sm-offset-4 col-sm-4">
  <div class="panel panel-default">
    <div class="panel-heading">
<h3>Editing Event</h3>
</div>
<div class="panel-body">

<%= form_for(@event) do |form| %>
	<h4>Host: <%= @event.creator.name %></h4>
	<%= render form %>
  <div class="actions">
    <%= form.submit class: "btn btn-default" %>
  </div>
<% end %>
</div>
</div>
